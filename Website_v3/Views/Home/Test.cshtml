@{
   ViewBag.Title = "";
   ViewBag.HeaderTitle = "";
   ViewBag.HeaderLine1 = "";
   ViewBag.HeaderLine2 = "Intelligent Mental Wellness Platform";
   ViewBag.TM_HeaderLine2 = "<span class=\"text-transform-none\">&trade;</span>";
   ViewBag.HeaderImage = "url(../img/headers/Dawn_of_Healing.jpg)";
   ViewBag.PageTitle = " ";
   ViewBag.PageSubTitle = "Saving Lives, One Mind at a Time";
   ViewBag.TM_PageSubTitle = "<span class=\"text-transform-none\">&trade;</span>";
   ViewBag.PageIntro = " ";        
}

<!-- ########## M A I N  C O N T E N T ########## -->
    <main data-zanim-timeline="{}" data-zanim-trigger="scroll">
        <!-- ########## H E A D E R ########## -->
        <section class="text-center overflow-hidden py-0" id="top">
            <div class="bg-holder overlay parallax" style="background-image:@ViewBag.HeaderImage;"></div>
            <div class="header-overlay"></div>
            <div class="container" data-zanim-xs='{"duration":"1.5","delay":"0.1"}'>
                <div class="d-flex align-items-center pt-10 pb-6">
                    <div class="header-text">
                        <div class="overflow-hidden">
                            <h1 class="display-3 text-400 fs-4 fs-md-5 font-weight-medium">@Html.Raw(ViewBag.HeaderTitle)</h1>
                            <p class="text-uppercase text-400 ls-2 mt-2 my-0 pb-0 pt-2 font-weight-medium">@ViewBag.HeaderLine1</p>
                            <p class="my-0 py-0"><span class="fs-7 logo-secondary-style-light">pr<span class="logo-primary-style-light">e</span>vidence<sup class="fs-3 font-weight-light">&reg;</sup></span></p>
                            <p class="text-uppercase fs-1 text-400 ls-2 my-0 py-0 font-weight-medium">@ViewBag.HeaderLine2@Html.Raw(ViewBag.TM_HeaderLine2)</p>
                        </div>
                        <partial name="_CallToActionLight" />
                        <partial name="_PageDown" />
                    </div>
                </div>
            </div>
            <partial name="_ClientBanner" />
        </section>
        <!-- / end  H E A D E R -->
        <!-- ########## C O N T E N T ########## -->
        <section class="text-center overflow-hidden py-0" id="Content">
            <section class="py-5" id="Content">
                <div class="container">
                    <partial name="_PageTitle" />
                </div>
            </section>
            <partial name="_Services" />
        </section>
        <partial name="_Testimonials" />
        <partial name="_ClientBanner" />
        <!-- / end  C O N T E N T -->
    </main>
<!-- / end  M A I N -->
